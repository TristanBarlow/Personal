  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'webserver'",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=rust/webserver/Cargo.toml",
          "--bin=webserver",
          "--package=webserver"
        ],
        "filter": {
          "name": "webserver",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceRoot}/rust/webserver"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'webserver'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=webserver",
          "--package=webserver"
        ],
        "filter": {
          "name": "webserver",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/rust/webserver"
    }
  ]
}